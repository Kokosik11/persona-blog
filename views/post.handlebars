<link rel="stylesheet" href="/assets/css/post.css">

<div class="post-page">
    <h2 class="post-heading">{{post.title}}</h2>
    <div class="post-image">
        <img src="{{staticURL post.image}}" alt="{{post.title}}">
    </div>
    <div class="post-content">
        {{{post.content}}}
    </div>
</div>